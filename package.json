    "@date-io/date-fns": "^2.10.8",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "babel": "^6.23.0",
    "babel-jest": "^26.6.3",
    "date-fns": "^2.19.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "jest": "^26.6.3",
    "mockdate": "^3.0.4",
    "prettier": "^2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "typescript": "^4.2.3"
  }
