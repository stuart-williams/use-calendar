  "keywords": [
    "react",
    "hooks",
    "use-calendar",
    "calendar"
  ],
  "main": "lib/use-calendar.js",
  "types": "lib/use-calendar.d.ts",
  "module": "esm/use-calendar.js",
  "sideEffects": false,
  "files": [
    "lib",
    "esm"
  ],
  "author": "Stuart Williams",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stuart-williams/use-calendar"
  },
  "bugs": {
    "url": "https://github.com/stuart-williams/use-calendar/issues"
  },
  "homepage": "https://github.com/stuart-williams/use-calendar#readme",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "build:lib": "tsc src/use-calendar.ts --declaration --outDir lib",
    "build:esm": "tsc src/use-calendar.ts --module esnext --moduleResolution node --outDir esm",
    "postversion": "git push && git push --tags"
