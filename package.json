    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "date-fns": "^2.16.1",
    "eslint": "^7.9.0",
    "framer-motion": "^2.9.4",
    "next": "10.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "typescript": "^4.0.5"
  }
